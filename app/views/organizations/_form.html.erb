<%= simple_form_for @organization, html: { multipart: true } do |f| %>
  <%= f.input :name, input_html: {class: "abc"}, label_html: {class: "hey"} %>
  <%= f.input :address %>
  <%= f.input :overview %>
  <%= f.input :employee_count %>
  <%= f.input :tech_team_size %>
  <%= f.input :website %>
  <%= f.input :twitter %>
  <%#= f.input :manager %>
  <% if current_user.admin == true %>
    <%= f.input :published, as: :boolean, checked_value: true, unchecked_value: false %>
  <% end %>
  <%= f.association :technologies, input_html: {class: "chosen-select"} %>
  <%= f.input :remote_logo_url, {label: "Upload Logo URL"} %>
  <label>Or Upload Logo Image File</label>
  <%= f.file_field :logo %>
  <%= f.check_box :remove_logo %> Remove Logo<br><br>
  <label>Add Your Organization's Photos</label>
  <%= f.file_field :image %>
  <%= f.file_field :image2 %>
  <%= f.file_field :image3 %>
  <%= f.file_field :image4 %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
